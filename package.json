{
  "name": "@koralle/vitest-axe",
  "version": "0.1.1",
  "description": "Custom Vitest matcher for testing accessibility with aXe. Forked from jest-axe.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koralle/vitest-axe.git"
  },
  "packageManager": "pnpm@9.15.9",
  "license": "MIT",
  "author": "koralle",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "lint": "eslint . --max-warnings=0 --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write '**/*.{ts,js}' --cache",
    "format-check": "prettier --check '**/*.{ts,js}' --cache",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "dependencies": {
    "axe-core": "^4.10.3",
    "chalk": "^5.4.1",
    "es-toolkit": "^1.37.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.15.17",
    "@vitest/eslint-plugin": "^1.1.44",
    "eslint": "^9.26.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.15.0",
    "jsdom": "^24.1.3",
    "lefthook": "^1.11.12",
    "prettier": "^3.5.3",
    "pretty-format": "^28.1.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "vitest": ">=2.1.9"
  },
  "engines": {
    "node": ">=18.20.8",
    "pnpm": ">=9.15.9",
    "npm": "please use pnpm!",
    "yarn": "please use pnpm!"
  }
}
